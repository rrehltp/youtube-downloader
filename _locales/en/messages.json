    "injGuideFeatSdSec1Title": {
        "message": "How to download a video?"
    },
    "injGuideFeatSdSec1Step1P1": {
        "message": "Click"
    },
    "injGuideFeatSdSec1Step1P2": {
        "message": "icon in the toolbar."
    },
    "injGuideFeatSdSec1Step2": {
        "message": "Wait for video details to load."
    },
    "injGuideFeatSdSec1Step3": {
        "message": "Click on your desired video quality from the list."
    },
    "injGuideFeatSdSec1Step4": {
        "message": "Your file download will start."
    },
    "injGuideFeatSdNote1P1": {
        "message": "You can"
    },
    "injGuideFeatSdNote1P2": {
        "message": "generate a QR code"
    },
    "injGuideFeatSdNote1P3": {
        "message": "and scan it to download the video on your phone."
    },
    "injGuideFeatSdNote2": {
        "message": "YouTube does not provide HD downloads for some videos, we use an external download service to for those videos."
    },
    "injGuideFeatSdNote3": {
        "message": "Dash format videos do not contain audio."
    },
